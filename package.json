{
  "name": "cordova-plugin-firebase-intercom",
  "version": "0.0.1",
  "description": "Google Firebase Intercom Integration Plugin",
  "scripts": {
    "commitmsg": "commitlint -E GIT_PARAMS --config ./.commitlintrc.yml",
    "clear": "rm -rf scripts",
    "build": "npm run clear && ./node_modules/.bin/tsc --p tsconfig.json",
    "lint": "./node_modules/.bin/tslint -p tsconfig.json",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts"
  },
  "main": "src/scripts/index.ts",
  "cordova": {
    "id": "cordova-plugin-firebase-intercom",
    "platforms": [
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roalcantara/cordova-plugin-firebase-intercom.git"
  },
  "keywords": [
    "cordova",
    "firebase",
    "intercom",
    "ecosystem:cordova",
    "cordova-android"
  ],
  "author": "Rogério R. Alcântara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roalcantara/cordova-plugin-firebase-intercom/issues"
  },
  "homepage": "https://github.com/roalcantara/cordova-plugin-firebase-intercom#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.7.2",
    "chai": "^4.1.2",
    "husky": "^3.0.4",
    "ts-node": "^8.3.0",
    "mocha": "^6.2.0",
    "tslint": "^5.11.0",
    "tslint-clean-code": "^0.2.7",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^3.0.1"
  }
}
